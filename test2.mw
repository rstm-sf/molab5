<Text-field prompt="&gt; " style="Maple Input" layout="Normal">find_max_and_plane := proc(obj, consts)
local bs, L, i, k, n, m, m_el, pos, tmp_plane, plane, res, eq:
res := maximize(obj, consts, NONNEGATIVE, 'NewConsts'):
bs := basis(NewConsts):
L := []: k := 1: n := nops(res): m := nops(bs):
for i from 1 to n do:
  tmp_plane := res[i]:
  if lhs(tmp_plane) = bs[k] then:
    L := [op(L), frac(rhs(tmp_plane))]:
    k := k + 1:
    if k &gt; m then: break: end if:
  end if:
end do:
m_el,pos := ListTools:-FindMaximalElement(L, position):
if m_el = 0 then:
<Font encoding="UTF-8">  printf(&quot;\320\227\320\260\320\264\320\260\321\207\320\260 \321\200\320\265\321\210\320\265\320\275\320\260!\134n&quot;);
</Font>  return res:
end if;
eq := rhs(NewConsts[pos]):
eq := -abs(coeffs(eq)[1] - floor(coeffs(eq)[1])) + map(proc(x)  abs(op(x)[1]-ceil(op(x)[1]))*op(x)[2]  end proc,eq);
return [res, eq]:
end proc:</Text-field>
</Input>
